{
	"name": "svelte5",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "eslint .",
		"postinstall": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.2.5",
		"@inlang/paraglide-js-adapter-sveltekit": "^0.4.0",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.36.0-next.4",
		"svelte": "^5.0.0-next.53",
		"svelte-check": "^3.6.3",
		"tslib": "^2.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.1.0",
		"vitest": "^1.2.2"
	},
	"type": "module",
	"packageManager": "yarn@1.22.19"
}
